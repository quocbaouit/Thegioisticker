        <div class="container">
            @*<h3>BÁO GIÁ TEM NHÃN STICKER ONLINE</h3>*@
            <div style="font-size:24px;font-weight:500">Báo Giá Tem Nhãn, Sticker Online</div>
            <div class="row">
                <div class="col-lg-7">
                    <div class="cart_container">
                        <div class="cart_items">
                            <ul class="cart_list">
                                <li class="cart_item clearfix">
                                    <h5>Vui lòng chọn loại chất liệu và nhập kích thước, số lượng</h5>
                                    <div class="input-group">
                                        <div class="input-group-prepend">
                                            <span class="input-group-text" id="">Chất Liệu</span>
                                        </div>
                                        <select ng-change="materialChange()" class="form-control classic ng-pristine ng-untouched ng-valid ng-not-empty" ng-model="shoppingCart.material" ng-options="opt as opt.label for opt in options"><option label="Decal sữa" value="object:6" selected="selected">Decal sữa</option><option label="Decal trong" value="object:7">Decal trong</option><option label="Decal giấy" value="object:8">Decal giấy</option><option label="Decal Kraft" value="object:9">Decal Kraft</option><option label="Decal xi bạc" value="object:10">Decal xi bạc</option><option label="Decal 7 màu" value="object:11">Decal 7 màu</option><option label="Tem bảo hành, tem vỡ,tem bế" value="object:12">Tem bảo hành, tem vỡ,tem bế</option><option label="Decal sữa cao cấp" value="object:13">Decal sữa cao cấp</option></select>
                                    </div>
                                    <div class="input-group">
                                        <div class="input-group-prepend">
                                            <span class="input-group-text" id="">Nhập kích thước</span>
                                        </div>
                                        <input num-only="" maxlength="9" ng-model="shoppingCart.width" type="text" placeholder="ngang(mm)" class="form-control ng-pristine ng-untouched ng-valid ng-isolate-scope ng-empty ng-valid-maxlength">
                                        <input num-only="" maxlength="9" ng-model="shoppingCart.height" type="text" placeholder="cao(mm)" class="form-control ng-pristine ng-untouched ng-valid ng-isolate-scope ng-empty ng-valid-maxlength">
                                    </div>

                                    <div class="input-group">
                                        <div class="input-group-prepend">
                                            <span class="input-group-text" id="">Số Lượng</span>
                                        </div>
                                        <input num-only="" maxlength="9" ng-model="shoppingCart.quantity" type="text" placeholder="nhập số lượng cần in" class="form-control ng-pristine ng-untouched ng-valid ng-isolate-scope ng-empty ng-valid-maxlength">
                                    </div>
                                    <div class="input-group">
                                        <div class="input-group-prepend">
                                            <span class="input-group-text" id="">Chọn Gia Công</span>
                                        </div>
                                        <select class="form-control classic ng-pristine ng-untouched ng-valid ng-not-empty" ng-model="shoppingCart.machining" ng-options="opt as opt.label for opt in options1"><option label="Cán màng" value="object:14" selected="selected">Cán màng</option><option label="Không cán màng" value="object:15">Không cán màng</option></select>
                                    </div>
                                    <div class="input-group">
                                        <div class="input-group-prepend">
                                            <span class="input-group-text" id="">Chọn Bế</span>
                                        </div>
                                        <select class="form-control classic ng-pristine ng-untouched ng-valid ng-not-empty" ng-model="shoppingCart.cut" ng-options="opt as opt.label for opt in options2"><option label="Bế thẳng" value="object:16" selected="selected">Bế thẳng</option><option label="Bế đặc biệt(hình tròn, hình khó)" value="object:17">Bế đặc biệt(hình tròn, hình khó)</option></select>
                                    </div>
                                </li>
                            </ul>
                        </div>

                    </div>
                </div>
                <div class="col-lg-5">
                    <div class="cart_container">
                        <div class="cart_items">
                            <ul class="cart_list">
                                <li class="cart_item clearfix  font-italic">
